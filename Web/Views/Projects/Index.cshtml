@model IEnumerable<Domain.Entities.Project>

<h2>Projects</h2>

<a href="@Url.Action("Create")">Create New Project</a>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var project in Model)
        {
            <tr>
                <td>@project.Name</td>
                <td>@project.Description</td>
            </tr>
        }
    </tbody>
</table>
