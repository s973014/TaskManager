@model Domain.Entities.Project

<h2>Удаление проекта</h2>

<p>Вы действительно хотите удалить проект <strong>@Model.Name</strong>?</p>

<form asp-action="Delete">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Удалить</button>
    <a class="btn btn-secondary" href="@Url.Action("Index", "Home")">Отмена</a>
</form>
